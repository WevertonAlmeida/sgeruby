<h1>Listing disciplinas</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Sigla</th>
    <th>Ementa</th>
    <th>Bibliografia</th>
    <th>Belongs to</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @disciplinas.each do |disciplina| %>
  <tr>
    <td><%= disciplina.nome %></td>
    <td><%= disciplina.sigla %></td>
    <td><%= disciplina.ementa %></td>
    <td><%= disciplina.bibliografia %></td>
    <td><%= disciplina.belongs_to %></td>
    <td><%= link_to 'Show', disciplina %></td>
    <td><%= link_to 'Edit', edit_disciplina_path(disciplina) %></td>
    <td><%= link_to 'Destroy', disciplina, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Disciplina', new_disciplina_path %>
