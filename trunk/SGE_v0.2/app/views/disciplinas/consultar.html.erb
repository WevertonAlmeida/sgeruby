<div align="center">
  <table align="center" cellspacing="2" cellpadding="0" border="0">
    <tr>
      <td height="50" colspan="2" align="center"><h1>Consultar disciplina</h1></td>
    </tr>
    <tr>
      <td height="50" colspan="2" align="center"><%= notice %></td>
    </tr>
    <tr>
      <td width="50%" height="50" align="right"><strong>Nome:</strong></td>
      <td width="50%" align="left"><%= @disciplina.nome %></td>
    </tr>
    <tr>
      <td height="50" align="right"><strong>Sigla:</strong></td>
      <td align="left"><%= @disciplina.sigla %></td>
    </tr>
    <tr>
      <td height="50" align="right"><strong>Ementa:</strong></td>
      <td align="left"><%= @disciplina.ementa %></td>
    </tr>
    <tr>
      <td height="50" align="right"><strong>Bibliografia:</strong></td>
      <td align="left"><%= @disciplina.bibliografia %></td>
    </tr>
    <tr>
      <td height="50" align="right"><strong>Curso:</strong></td>
      <td align="left"><%= @disciplina.curso.nome %></td>
    </tr>
    <tr>
      <td height="60" colspan="2" align="center"><%= link_to 'Editar', editar_disciplina_path(@disciplina) %></td>
    </tr>
    <tr>
      <td height="10" colspan="2" align="center"><%= link_to 'Voltar', :back %></td>
    </tr>
  </table>
</div>

