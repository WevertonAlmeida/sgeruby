<table cellspacing="2" cellpadding="0" border="0">
  <tr>
    <td colspan="2" align="center" height="50"><h1>Consultar aluno</h1></td>
  </tr>
  <tr>
    <td colspan="2" align="center" style="color: green"><%= notice %></td>
  </tr>
  <tr>
    <td height="30" align="right"><b>Nome:</b></td>
    <td align="left"><%=h @usuario.nome %></td>
  </tr>
  <tr>
    <td height="30" align="right"><b>Senha:</b></td>
    <td align="left"><%=h @usuario.senha %></td>
  </tr>
  <tr>
    <td height="30" align="right"><b>Certificado de Conclus&atilde;o/Dispansa do Servi&ccedil;o Militar:</b></td>
    <td align="left"><%=h @aluno.cert_militar %></td>
  </tr>
  <tr>
    <td colspan="2" align="center" height="50"><%= link_to "Matricular Aluno", matricular_aluno_path(@aluno.id) %> |
<%= link_to "Editar", editar_aluno_path(@aluno) %> | <%= link_to "Voltar", :back %>
    </td>
  </tr>
</table>
